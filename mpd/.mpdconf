follow_outside_symlinks "yes"
follow_inside_symlinks "yes"
db_file "/home/sulami/.mpd.db"
sticker_file "/home/sulami/.mpd.sticker"
# log_file "/var/log/mpd.log"
bind_to_address "127.0.0.1"

music_directory "/home/sulami/Music"

audio_output {
    type "alsa"
    name "alsa"
}

